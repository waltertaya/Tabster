{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Tabster",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
          "16": "images/icon16.png",
          "48": "images/icon48.png",
          "128": "images/icon128.png"
        }
    },
    "permissions": [
    "storage",
    "tabs",
    "tabGroups"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}